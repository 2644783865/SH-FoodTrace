<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - 食品溯源平台管理</title>
    @Styles.Render("~/Content/jquery-easyui-1.4.5/themes/default/easyui.css")
    @Styles.Render("~/Content/jquery-easyui-1.4.5/themes/icon.css")
    @Scripts.Render("~/Content/jquery-easyui-1.4.5/jquery.min.js")
    @Scripts.Render("~/Content/jquery-easyui-1.4.5/jquery.easyui.min.js")
    @Scripts.Render("~/Content/jquery-easyui-1.4.5/locale/easyui-lang-zh_CN.js")
    <script type="text/javascript" src="~/Scripts/My97DatePicker/WdatePicker.js"></script>
    <script src="~/Scripts/base/amyUtils.js"></script>
    <style type="text/css">
        ul {
            list-style-type: none;
            padding: 0px;
            margin: 0px;
        }

            ul li {
                padding: 8px;
            }

                ul li a {
                    text-decoration: none;
                    color: black;
                    font-family: Arial;
                }

        a:hover {
        }
    </style>
</head>
<body class="easyui-layout">
        @RenderBody()
    @RenderSection("scripts",required:false)
    <script type="text/javascript">
        $(function () {
            $(".panel-body").css("border", "none");
        })
    </script>
</body>
</html>
